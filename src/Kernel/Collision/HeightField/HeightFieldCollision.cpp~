#include <HeightFieldCollision.h>
/*****************************************************************************************************/
/*****************************************************************************************************/
HeightFieldCollision::HeightFieldCollision():ObjectCollision()
{
}
/*****************************************************************************************************/
HeightFieldCollision::HeightFieldCollision(float elast):ObjectCollision(elast,0,false)
{
}
/*****************************************************************************************************/
HeightFieldCollision::HeightFieldCollision(const HeightFieldCollision& H):ObjectCollision(H)
{
}
/*****************************************************************************************************/
HeightFieldCollision::~HeightFieldCollision()
{
}
/**********************************************************************************/
/**********************************************************************************/
void HeightFieldCollision::display(GLenum mode, GLenum raster, Vector3 color)
{
	glPolygonMode(mode, raster);
	HeightField::display(color);
}
/**********************************************************************************/
void HeightFieldCollision::displayNormales(Vector3 color)
{
	HeightField::displayNormale(color);
}
/*****************************************************************************************************/
/*****************************************************************************************************/
